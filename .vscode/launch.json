{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Demo OCS",
      "type": "chrome",
      "request": "launch",
      "preLaunchTask": "start demo ocs",
      "postDebugTask": "Terminate All Tasks",
      "url": "http://localhost:4200/",
      "timeout": 60000,
      "webRoot": "${workspaceFolder}",
      "sourceMapPathOverrides": {
        "webpack:/*": "${webRoot}/*",
        "/./*": "${webRoot}/*",
        "/src/*": "${webRoot}/*",
        "/*": "*",
        "/./~/*": "${webRoot}/node_modules/*"
      }
    },
    {
      "name": "Demo WC",
      "type": "chrome",
      "request": "launch",
      "preLaunchTask": "start demo wc",
      "postDebugTask": "Terminate All Tasks",
      "url": "http://localhost:4200/",
      "timeout": 60000,
      "webRoot": "${workspaceFolder}",
      "sourceMapPathOverrides": {
        "webpack:/*": "${webRoot}/*",
        "/./*": "${webRoot}/*",
        "/src/*": "${webRoot}/*",
        "/*": "*",
        "/./~/*": "${webRoot}/node_modules/*"
      }
    }
  ]
}